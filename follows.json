    },
    "urls": {
      "type": "array",
      "title": "The followed URLs",
      "items": {
        "type": "string",
        "format": "uri",
        "examples": [
          "dat://beakerbrowser.com"
        ]
      }
    }
  },
  "additionalProperties": false
}